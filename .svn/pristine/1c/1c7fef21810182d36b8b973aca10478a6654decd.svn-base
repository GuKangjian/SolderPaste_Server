var dataView;
var indexArr = [1, 2, 3];
function initdataView(){

        dataView = new GC.Spread.Views.DataView(document.getElementById('grid1'), data, columns, new GC.Spread.Views.Plugins.GridLayout({
            rowHeight: 100,
            colHeaderHeight: 100,
            rowTemplate: '#rowTmpl',
            showRowHeader: false,  //左侧边栏
            selectionMode: 'row',
            editUnit: 'cell',
            allowEditing: true,
            // allowAutoAddRow: true,
            autoAddRowPosition: 'bottom'
        }));

        dataView.editing.addHandler(function(sender, args) {

            // console.log(args)
            // console.log(args.isNewRow)

            if (args.status === 'beforeStartEditing') {

                if(args.isNewRow) {
                    args.oldItem = {
                        id: data.length,
                        Product: 'ww',
                        country: null,
                        date: new Date(),
                        amount: 0,
                        active: false
                    };    
                }else{

                    if (indexArr.indexOf(args.oldItem.id) != -1) {

                        args.cancel = true;
                    }

                }
            }

            // if (args.status === 'beforeStartEditing' && indexArr.indexOf(args.oldItem.id) != -1) {

            //     args.cancel = true;

            // }
            if (args.status === 'endEditing' && args.isNewRow || args.status === 'cancelEditing') {  //编辑完不自动增加一行

                if (dataView.options.allowAutoAddRow) {
                    dataView.options.allowAutoAddRow = false;
                    dataView.invalidate();   //使无效 (rebuildDOMTree)Invalidates the entire control.saveBatchEdit(callback)setSelections(sourceRow, columns)Set selection.
                }
            }

        });

        $('#btnAddNew').on('click', function() {

            var isEditing = !!document.getElementById(dataView.uid + '-inline-editing-area');
            if (!isEditing) {
                dataView.options.allowAutoAddRow = true;
                dataView.invalidate();
                var autoRow = $('#' + dataView.uid + '-autorow');
                // autoRow.find('[data-action="save"]').css('display', 'inline-block');
                // autoRow.find('[data-action="cancel"]').css('display', 'inline-block');
                // autoRow.find('[data-action="edit"]').css('display', 'none');
                // autoRow.find('[data-action="delete"]').css('display', 'none');
                dataView.startEditing(0);
            }
        });

        //focus data.view by default
        document.querySelector('#grid1').focus();

}


function productEditor(options) {
    this.options = options;
}

productEditor.prototype = {
    init: function() {
        var options = this.options;
        var container = options.container;
        var list = ['ABS', 'AS', 'BE', 'CC-DA', 'CM', 'DCU', 'EC', 'ED'];  //后台获取
        var len = list.length;
        var InnerHTML = '';
        for(i=0;i<len;i++){
            InnerHTML += '<option value="'+ list[i] +'">'+ list[i] +'</option>'
        }
        container.innerHTML = '<select id="" class="product form-control gc-inline-editor">'+InnerHTML+'</select>';
        this.product = container.querySelector("select.product");
        // this.focus();
    },

    focus: function() {
        this.product.focus();
    },

    serialize: function() {

        return this.product.value;
    }
};

