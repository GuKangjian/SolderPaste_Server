@charset "utf-8";

/* CSS Document */

* {
    margin: 0;
    padding: 0;
}

ol, ul {
    list-style: none;
}

ul.btn-item,
ul.items {
    float: right;
    background: transparent;
    margin-top: 0px;
    margin-bottom: 0;
    font-size: 12px;
    /*padding: 7px 5px;*/
    border-radius: 2px;
    overflow: hidden;
}

.btnbox{
    float: right;
    background: transparent;
    bottom: 0;
    /*margin-right: 10px;*/
    /*margin-right: 15px;*/
}

ul.portal {
    width: 1200px;
    margin: 10% auto;
}

ul.portal li {
    list-style: none;
    float: left;
    width: 30%;
    height: 250px;
    background: inherit;
    background-color: rgba(25, 158, 216, 0.349019607843137);
    padding: 50px 0;
    margin: 10px 10px;
    border-radius: 8px;
    text-align: center;
    font-size: 24px;
}

.portal p {
    padding-top: 10px;
}

.portalmenu{float: left;margin: 10px 2% 0 45%;width: 30%;}

.portalmenu img{width: 15px;}

.portalmenu li{    
    list-style: none;
    display: inline-block;
    padding: 0 10px;
    position: relative;
    width: 38px;
}

.portalmenu p{display: none;white-space: nowrap;}
 /*小图标hover展示说明*/
.portalmenu li:nth-child(1):hover p{display: block;position: absolute;left: -5px;top: 18px;}  
.portalmenu li:nth-child(2):hover p{display: block;position: absolute;left: -18px;top: 18px;}
.portalmenu li:nth-child(3):hover p{display: block;position: absolute;left: -15px;top: 18px;}
.portalmenu li:nth-child(4):hover p{display: block;position: absolute;left: 2px;top: 18px;}
.portalmenu li:nth-child(5):hover p{display: block;position: absolute;left: -20px;top: 18px;}
.portalmenu li:nth-child(6):hover p{display: block;position: absolute;left: 2px;top: 18px;}
.portalmenu li:nth-child(7):hover p{display: block;position: absolute;left: 4px;top: 18px;}
.portalmenu li:nth-child(8):hover p{display: block;position: absolute;left: -15px;top: 18px;}
.portalmenu li:nth-child(9):hover p{display: block;position: absolute;left: -2px;top: 18px;}



.categories{width: 1200px;margin: 10% auto;}
.categories ul{margin: 0 -1%;}
.categories ul li {
    list-style: none;
    float: left;
    width: 48%;
    height: 160px;
    background: inherit;
    background-color: rgba(25, 158, 216, 0.349019607843137);
    padding: 50px 0;
    margin: 0 10px;
    border-radius: 8px;
    text-align: center;
    font-size: 24px;
}

.categories p {
    padding-top: 10px;
}

.contentwp {
    width: 100%;
}

ul.btn-item li,
ul.items li,
ul.btnbox li,
.editbtn li {
    list-style: none;
    display: inline-block;
    -moz-box-shadow: none;
    -webkit-box-shadow: none;
    box-shadow: none;
    box-sizing: border-box;
    word-wrap: break-word;
}

.part1 ,.part2 ,.part3 ,.part4 {width: 100%;}

