    $('#grid-edit-unit').change(function() {
        var sel = document.getElementById("grid-edit-unit");
        var value = sel.options[sel.selectedIndex].value;
        var options = dataView.layoutEngine.options;
        options.editUnit = value;
        options.selectionUnit = value;

