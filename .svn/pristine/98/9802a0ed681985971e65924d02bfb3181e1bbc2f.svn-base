        time_canvas = document.querySelectorAll(".time-graph-canvas");

        $.each(time_canvas,function (index, v) {

            drawMain(time_canvas[index], 70, "#85d824", "#eef7e4"); 

        });

        
        $('.datetime').datetimepicker({
            format: 'yyyy-mm-dd',
            language: 'en',
            weekStart: 1,
            todayBtn: 1,
            autoclose: 1,
            todayHighlight: 1,
            startView: 2,
            minView: 2,
            forceParse: 0
        });

        //Date range picker
        $('#timeperiod').daterangepicker();

    });
</script>
</body>

</html>