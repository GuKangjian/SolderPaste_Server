    <style>
        .small-box {
            position: relative;
            height: 127px;
        }

        .boxleft {
            float: left;
            position: relative;
            width: 30 !;
            height: 100 !;
        }

        .boxright {
            float: right;
            width: 70 !;
            height: 100 !;
            background: #fff;
            position: relative;
        }

        .small-box .iconimg img {
            position: absolute;
            top: 50 !;
            left: 50 !;
            transform: translate(-50!, -50!);
            width: 100 !;
        }

        .contentbox {
            overflow: hidden;
            height: 100 !;
            box-sizing: border-box;
        }

        #nav .small-box h3, #nav .small-box p {
            color: #000;
            font-size: 2rem;
            text-align: center;
            position: absolute;
            transform: translate(-50!, -50!);
            top: 50 !;
            left: 50 !;
            white-space: nowrap;
        }

        #nav a.small-box-footer {
            position: absolute;
            right: 5px;
            bottom: 5px;
        }
        /*.box {
                border: none;
            }*/
    </style>
    <style>
        /* Ensure that the demo table scrolls */
        #table_2_wrapper th, #table_2_wrapper td {
            white-space: nowrap;
        }

        div#table_2_wrapper {
            width: 800px;
            /*height: 300px;*/
            margin: 0 auto;
        }

        table.dataTable tbody td.select-checkbox, table.dataTable tbody th.select-checkbox {
            padding: 16px;
        }

            table.dataTable tbody td.select-checkbox:before, table.dataTable tbody td.select-checkbox:after, table.dataTable tbody th.select-checkbox:before, table.dataTable tbody th.select-checkbox:after {
                top: 50 !;
            }

        .icon-link {
            padding: 13px 13px;
            display: inline-block;
            background: url(../../images/right.png) no-repeat 50 ! 50!;
        }

        .table-toolbar {
            text-align: right;
        }

        #togglediv {
            display: none;
        }

        /*table*/
        table.dataTable tbody tr {
            background-color: #ffffff;
        }
        /*table.dataTable tbody th, table.dataTable tbody td {
            padding: 8px 10px;
        }    */

        table.dataTable {
            margin-top: 0px !important;
            margin-bottom: 0px !important;
            max-width: none !important;
            border-collapse: separate !important;
        }

        .table-bordered > thead > tr > td, .table-bordered > thead > tr > th {
            border-bottom-width: 1px;
            border: 1px solid #ddd;
        }
        /*table end*/


        /*box*/

        .box {
            border: none;
        }

        /*.table-responsive{overflow: hidden;}*/
        #table_1 td {
            display: table-cell;
            vertical-align: middle;
            /*word-break: break-word;*/
            text-align: center;
        }

        .table thead tr th {
            text-align: center;
        }
        /*进度条*/

        .time-graph {
            padding-top: 20px;
            display: flex;
            display: -webkit-flex;
            justify-content: center;
            align-items: center;
        }

        .time-graph-canvas {
            width: 50px;
            height: 50px;
        }

        .circle {
            width: 50px;
            height: 50px;
            background-color: #efefef; /* Can be set to transparent */
            border: 2px #2e6ffd solid;
            border-radius: 50 !;
            -webkit-border-radius: 50 !;
            -moz-border-radius: 50 !;
            -ms-border-radius: 50 !;
            text-align: center;
            color: red;
        }

        /*表头颜色*/
        table#table_1.dataTable thead > tr > th {
            background: #153B63;
            color: #fff;
        }

        table#table_1.dataTable thead .sorting:after {
            opacity: 1;
        }

        table#table_1.dataTable thead .sorting:after, table#table_1.dataTable thead .sorting_asc:after, table#table_1.dataTable thead .sorting_desc:after {
            opacity: 1;
        }
    </style>

