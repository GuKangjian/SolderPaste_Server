    <script>
        $(function () {
            LoadLineNo();
            $("#btnSearch").click(function () {
                GetRequestData(1, 5, $("#lineNo option:selected").text());
                GetResponseData(1, 5, $("#lineNo option:selected").text());
            })
            //翻页事件
            $('#table_1').on('page-change.bs.table', function (e, number, size) {
                GetRequestData(number, size, $("#lineNo option:selected").text());
            });
            $('#table_2').on('page-change.bs.table', function (e, number, size) {
                GetResponseData(number, size, $("#lineNo option:selected").text());
            });
        })

        function operateFormatter_1(value, row, index) {
            return [
                '<a  style="text-decoration:none">' + row.AduitStatus + '</a> ',
            ].join('');
        }

        function operateFormatter_2(value, row, index) {
            return [
                '<a  style="text-decoration:none">' + row.AduitStatus + '</a> ',
            ].join('');
        }
        function LoadLineNo() {
            $.ajax({
                type: "get",
                cache: true,
                asyn: false,
                url: "/Home/LineNo",
                dataType: "json",
                success: function (data) {
                    var list = eval("(" + data + ")");
                    var vocaspec = (" <option disabled selected>-- All --</option>");
                    for (var i = 0; i < list.length; i++) {
                        vocaspec += ("<option value=\"" + list[i].Id + "\">" + list[i].LineName);
                        vocaspec += ("</option>");
                    }
                    $("#lineNo").html(vocaspec);
                }
            })
        }
        function GetRequestData(number, size, line) {
            $("#table_1").bootstrapTable("showLoading");
            $.ajax({
                type: "POST",
                cache: true,
                asyn: false,
                data: { "LineNo": line, "currentPage": number, "size": size, "sign": "1", "startTime": $("#starttime").val(), "endTime": $("#endtime").val() },
                url: "/Home/GetDifferDataByDate",
                dataType: "text",
                success: function (data) {
                    if (data != null && data != '' && data.rows != "") {
                        var target = $.parseJSON(data);
                        $("#table_1").bootstrapTable("load", target);
                    }
                    $("#table_1").bootstrapTable("hideLoading");

                }
            })
        }

        function GetResponseData(number, size, line) {
            $("#table_2").bootstrapTable("showLoading");
            $.ajax({
                type: "POST",
                cache: true,
                asyn: false,
                data: { "LineNo": line, "currentPage": number, "size": size, "sign": "2", "startTime": $("#starttime").val(), "endTime": $("#endtime").val() },
                url: "/Home/GetDifferDataByDate",
                dataType: "text",
                success: function (data) {
                    if (data != null && data != '' && data.rows != "") {
                        var target = $.parseJSON(data);
                        $("#table_2").bootstrapTable("load", target);
                    }

                    $("#table_2").bootstrapTable("hideLoading");

                }
            })
        }
    </script>
